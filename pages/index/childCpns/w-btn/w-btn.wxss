/* pages/index/childCpns/w-btn/w-btn.wxss */
.bView {
  /* 设置水平居中 */
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bBtn {
  height: 50px;
  width: 80%;
  background-color:  #fff;
  border: 1px solid #eee;
  border-radius: 50px;
  display: flex;
  position: fixed;
  /* top: 300rpx; */
}

.bBtn button {
  flex: 1;
  line-height: 35px;
  text-align: center; 
  border-radius: 50px; 
  border: none;
  background-color: transparent;
}